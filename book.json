{
  "description": "{{description}}",
  "author": "{{author}}",
  "title": "{{description}}",
  "language": "zh-hans",
  "links": {
    "gitbook": false,
    "sharing": {
      "google": false,
      "facebook": false,
      "twitter": false,
      "all": false
    }
  },
  "plugins": [
    "-lunr",
    "-search",
    "search-plus",
    "anchors",
    "page-footer-ex",
    "expandable-chapters-small",
    "splitter",
    "anchor-navigation-ex",
    "favicon",
    "copy-code-button",
    "insert-logo",
    "toggle-chapters"
  ],
  "pluginsConfig": {
    "theme-default": {
      "showLevel": false
    },
    "page-footer-ex": {
      "copyright": "©{{year}} {{author}}. All Rights Reserved",
      "markdown": true,
      "update_label": "updated：",
      "update_format": "YYYY-MM-DD HH:mm:ss"
    },
    "insert-logo": {
      "url": "/assets/img/logo.png"
    },
    "favicon": {
      "shortcut": "/assets/img/favicon.ico",
      "bookmark": "/assets/img/favicon.ico"
    }
  },
  "styles": {
    "website": "assets/style/website.css"
  },
  "expandable-chapters-small": {}
}
